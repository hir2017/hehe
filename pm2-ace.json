{
    "apps": [
    {
        "name": "upexchange-acefront",
        "script": "./server.js",
        "instances": 1,
        "exec_mode": "fork",
        "log_date_format": "YYYY-MM-DD HH:mm:ss",
        "error_file": "./logs/error.log",
        "out_file": "./logs/out.log",
        "log_file": "./logs/all.log",
        "combine_logs": true,
        "env":
        {
            "NODE_ENV": "development"
        },
        "env_production":
        {
            "NODE_ENV": "production"
        }
    }]
}